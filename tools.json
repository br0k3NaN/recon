{
    "tools":[
        {
            "name":"subfinder",
            "install":"go install -v github.com/projectdiscovery/subfinder/v2/cmd/subfinder@latest"
        },
        {
            "name":"amass",
            "install":"go install -v github.com/OWASP/Amass/v3/...@master"
        },
        {
            "name":"assetfinder",
            "install":"go install -v github.com/tomnomnom/assetfinder@latest"
        },
        {
            "name":"gobuster",
            "install":"go install -v github.com/OJ/gobuster/v3@latest"
        },
        {
            "name":"nmap",
            "install":"sudo apt-get install -y nmap"
        },
        {
            "name":"sqlmap",
            "install":"sudo apt-get install -y sqlmap"
        },
        {
            "name":"httpx",
            "install":"go install -v github.com/projectdiscovery/httpx/cmd/httpx@latest"
        },
        {
            "name":"nuclei",
            "install":"go install -v github.com/projectdiscovery/nuclei/v2/cmd/nuclei@latest"
        },
        {
            "name":"ffuf",
            "install":"go install -v github.com/ffuf/ffuf@latest"
        },
        {
            "name":"anew",
            "install":"go install -v github.com/tomnomnom/anew@latest"
        },
        {
            "name":"gau",
            "install":"go install github.com/lc/gau/v2/cmd/gau@latest"
        },
        {
            "name":"notify",
            "install":"go install -v github.com/projectdiscovery/notify/cmd/notify@latest",
            "commands":[
                {
                    "Moving to /usr/local/bin":"mv /root/go/bin/notify /usr/local/bin/notify"
                }
            ]
        }
    ]
}
