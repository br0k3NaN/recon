{
    "tools":[
        {
            "name":"subfinder",
            "install":"go get -v github.com/projectdiscovery/subfinder/v2/cmd/subfinder@latest"
        },
        {
            "name":"amass",
            "install":"go get -u github.com/OWASP/Amass/v3/...@master"
        },
        {
            "name":"assetfinder",
            "install":"go get -u github.com/tomnomnom/assetfinder"
        },
        {
            "name":"gobuster",
            "install":"go install github.com/OJ/gobuster/v3@latest"
        },
        {
            "name":"nmap",
            "install":"sudo apt-get install -y nmap"
        },
        {
            "name":"sqlmap",
            "install":"sudo apt-get install -y sqlmap"
        },
        {
            "name":"httpx",
            "install":"go install -v github.com/projectdiscovery/httpx/cmd/httpx@latest"
        },
        {
            "name":"nuclei",
            "install":"go install -v github.com/projectdiscovery/nuclei/v2/cmd/nuclei@latest"
        },
        {
            "name":"ffuf",
            "install":"go install github.com/ffuf/ffuf@latest"
        }
    ]
}
